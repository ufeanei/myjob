<div class='contain'>
  <div>
    <br>
    <h3 class='text-center'>All categories</h3>
    <br>
  </div>
<div class=' col-md-4 col-md-offset-4 well category-list'>
  <table class='col-md-12 table table-bordered table-hover '>
    <thead class="thead-default">
      <th>Job categories</th>
      <th>manage</th>
    </thead>
    <% @cat.each do |cat| %>
    <tr>
      <td><%= cat.name %></td>
      <td> <%= link_to 'Edit', edit_category_path(cat), class: 'btn btn-info btn-lg edit-btn' %></td>
    </tr>
    <% end %>
  </table>
</div>
</div>