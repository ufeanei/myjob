<div class="contain">
  <div class="col-xs-12 col-sm-10 col-sm-offset-1 col-md-10 col-md-offset-1">
    <div class="row">
      <aside class="col-md-4 col-sm-5 well dashboard-menu"> <!--styles for dashboard-menu found in myjobs.css.scss -->
        <h2>Dashboard</h2>
        <hr/>
        <div class='btn-group-vertical col-md-12'>          
         <%= link_to 'Mine annonser', my_jobs_dashboard_path, class: 'btn btn-default btn block text-left'  %>
          <%= link_to 'Mine søknader', my_applications_dashboard_path, class: 'btn btn-default text-left'  %>
          <%= link_to 'Invitasjoner fra jobb eiere', jobs_won_dashboard_path, class: 'btn btn-default text-left'  %>
          <%= link_to 'Mine vurderinger', user_path(current_user), class: 'btn btn-default text-left'  %>

          <%= link_to rate_your_helpers_dashboard_path , class: 'btn btn-default text-left' do %>
